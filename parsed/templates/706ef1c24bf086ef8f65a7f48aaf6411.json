{"components/common/quick-search":"<div class=\"dropdown dropdown--quickSearch desktop\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n\t<a href=\"#\" class=\"quickSearch-mobileToggle\" title=\"Submit Search\">\r\n\t\t<span class=\"mobileMenu-toggleIcon\"></span>\r\n\t</a>\r\n\t<form class=\"form\" action=\"{{urls.search}}\">\r\n\t    <!--<div class=\"search-form-wrapper\">-->\r\n    \t\t<!--<fieldset class=\"form-fieldset\">-->\r\n    \t\t<!--\t<div class=\"form-field\">-->\r\n    \t\t<!--\t\t<label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>-->\r\n    \t\t<!--\t\t<input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">-->\r\n    \t\t<!--\t\t <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\"><svg class=\"header-icon search-icon\"><use xlink:href=\"#icon-search\"></use></svg> -->\r\n    \t\t<!--\t</div>-->\r\n    \t\t<!--</fieldset>-->\r\n\t\t<!--</div>-->\r\n\t\t<div class=\"search-icon submit\">\r\n\t\t    \r\n\t\t    <!--<a href=\"#search\" class=\"search-form-tigger btn btn-success\"  data-toggle=\"search-form\"><span class=\"material-icons\">search</span></a>-->\r\n\t\t    <form class=\"search-form\" id=\"\" action=\"{{urls.search}}\">\r\n              <fieldset class=\"form-fieldset\">\r\n        \t\t<div class=\"form-field\">\r\n        \t\t   <!--<div class=\"flexbox\">-->\r\n             <!--       <div class=\"search\">-->\r\n             <!--           <input type=\"text\" class=\"form-input\" data-search-quick name=\"search_query\" id=\"\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\">-->\r\n             <!--       </div>-->\r\n             <!--      </div>-->\r\n                 <!--<span class=\"input-group-addon\" id=\"basic-addon2\"><i class=\"fa fa-search\" aria-hidden=\"true\"></i>-->\r\n                 <!--</span>-->\r\n                 <!-- <span class=\"input-group-addon search-close\" id=\"basic-addon2\"><i class=\"fa fa-window-close\" aria-hidden=\"true\"></i>\r\n                 </span> --> \r\n                     <!-- partial:index.partial.html -->\r\n                    <div class=\"flexbox\">\r\n                      <div class=\"search\">\r\n                        <div>\r\n                          <input type=\"text\" data-search-quick name=\"search_query\" id=\"\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\">\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    <!-- partial -->\r\n                </div>\r\n              </fieldset>\r\n            </form>\r\n\t\t    \r\n\t\t <!--   <button title=\"Submit Search\" class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\">-->\r\n\t\t\t<!--\t<span class=\"material-icons\">search</span>-->\r\n\t\t\t\t<!--{{lang \"search.quick_search.input_label\"}}-->\r\n\t\t\t<!--</button>-->\r\n\t\t\t\r\n\t\t\t<!--<button title=\"Submit Search\" class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang \"search.quick_search.input_label\"}}\">-->\r\n\t\t\t<!--\t<span class=\"material-icons\">search</span>-->\r\n\t\t\t<!--\t{{lang \"search.quick_search.input_label\"}}-->\r\n\t\t\t<!--</button>-->\r\n\t\t\t\r\n\t\t</div>\r\n\t</form>\r\n\t<section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n</div>\r\n\r\n\r\n<style>\r\n.flexbox {\r\n  /*background: linear-gradient(155deg, #55c7ed, #2bb3e0, #3bc1ed);*/\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.search {\r\n  margin: -10px 20px 20px;\r\n}\r\n\r\n.search > div {\r\n  display: inline-block;\r\n  position: relative;\r\n  filter: drop-shadow(0 1px #0091c2);\r\n}\r\n\r\n.search > div:after {\r\n  content: \"\";\r\n  background: white;\r\n  width: 4px;\r\n  height: 12px;\r\n  position: absolute;\r\n    top: 22px;\r\n    right: -2px;\r\n  transform: rotate(135deg);\r\n}\r\n\r\n.search > div > input {\r\n  color: white;\r\n  font-size: 16px;\r\n  background: transparent;\r\n    width: 30px;\r\n    height: 30px;\r\n    padding: 10px;\r\n  border: solid 3px white;\r\n  outline: none;\r\n  border-radius: 35px;\r\n  transition: width 0.5s;\r\n}\r\n\r\n.search > div > input::-moz-placeholder {\r\n  color: #efefef;\r\n  opacity: 0;\r\n  -moz-transition: opacity 150ms ease-out;\r\n  transition: opacity 150ms ease-out;\r\n}\r\n\r\n.search > div > input:-ms-input-placeholder {\r\n  color: #efefef;\r\n  opacity: 0;\r\n  -ms-transition: opacity 150ms ease-out;\r\n  transition: opacity 150ms ease-out;\r\n}\r\n\r\n.search > div > input::placeholder {\r\n  color: #efefef;\r\n  opacity: 0;\r\n  transition: opacity 150ms ease-out;\r\n}\r\n\r\n.search > div > input:focus::-moz-placeholder {\r\n  opacity: 1;\r\n}\r\n\r\n.search > div > input:focus:-ms-input-placeholder {\r\n  opacity: 1;\r\n}\r\n\r\n.search > div > input:focus::placeholder {\r\n  opacity: 1;\r\n}\r\n\r\n.search > div > input:not(:-moz-placeholder-shown) {\r\n  width: 250px;\r\n}\r\n\r\n.search > div > input:not(:-ms-input-placeholder) {\r\n  width: 250px;\r\n}\r\n\r\n.search > div > input:focus,\r\n.search > div > input:not(:placeholder-shown) {\r\n  width: 250px;\r\n}\r\n@media(max-width:767px){\r\n    .search {\r\n      margin: -5px 0px 20px;\r\n    }\r\n}\r\n\r\n@media(max-width:991px){\r\n\r\n    .search > div > input {\r\n        background: #39bbc9 !important;\r\n    }\r\n    \r\n    .header-logo>a {\r\n        z-index: 0;\r\n    }\r\n}\r\n\r\n</style>"}
