ut;\r\n  transition: opacity 150ms ease-out;\r\n}\r\n\r\n.search > div > input::placeholder {\r\n  color: #efefef;\r\n  opacity: 0;\r\n  transition: opacity 150ms ease-out;\r\n}\r\n\r\n.search > div > input:focus::-moz-placeholder {\r\n  opacity: 1;\r\n}\r\n\r\n.search > div > input:focus:-ms-input-placeholder {\r\n  opacity: 1;\r\n}\r\n\r\n.search > div > input:focus::placeholder {\r\n  opacity: 1;\r\n}\r\n\r\n.search > div > input:not(:-moz-placeholder-shown) {\r\n  width: 250px;\r\n}\r\n\r\n.search > div > input:not(:-ms-input-placeholder) {\r\n  width: 250px;\r\n}\r\n\r\n.search > div > input:focus,\r\n.search > div > input:not(:placeholder-shown) {\r\n  width: 250px;\r\n}\r\n@media(max-width:767px){\r\n    .search {\r\n      margin: -5px 0px 20px;\r\n    }\r\n}\r\n\r\n@media(max-width:991px){\r\n\r\n    .search > div > input {\r\n        background: #39bbc9 !important;\r\n    }\r\n    \r\n    .header-logo>a {\r\n        z-index: 0;\r\n    }\r\n}\r\n\r\n</style>","components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" title=\"{{name}}\" rel=\"noopener\">\r\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\r\n                    {{name}}\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n"}
